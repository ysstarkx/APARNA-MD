{
  "name": "whatsapp-bot (APARNA-MD)",
  "description": " Whatsapp bot.",
  "keywords": [
    "whatsapp bot"
  ],
  "repository": "https://github.com/MIDHUN-NOXZ/APARNA-MD",
  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "session id",
      "required": true,
      "value": ""
    },
    "SUDO": {
      "description": "admin",
      "required": true,
      "value": "null"
    },
    "MODE": {
      "description": "mode",
      "required": true,
      "value": "public"
    },
    "ALWAYS_ONLINE": {
      "description": "show bot number last seen as online",
      "required": false,
      "value": "false"
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku app name, same as above entered",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Heroku account api key, https://dashboard.heroku.com/account",
      "required": true
    },
    "LOG_MSG": {
      "description": "Show whatsapp msgs in log",
      "required": false,
      "value": "false"
    },
    "PREFIX": {
      "description": "prefix for bot. all so can be null or false multi prefix",
      "required": true,
      "value": "."
    },
    "BOT_INFO": {
      "description": "botname;ownername;ownernumber;image url",
      "required": true,
      "value": "𝑌𝑆 𝛭𝛪𝐷𝛨𝑈𝛮;𝛥𝛲𝛥𝑅𝛮𝛥 𝛭𝐷;917012751946;https://i.imgur.com/LgaENJd.jpeg"
    },
    "STICKER_DATA": {
      "description": "sticker package name",
      "required": true,
      "value": "𝑌𝑆 𝛭𝛪𝐷𝛨𝑈𝛮;𝛥𝛲𝛥𝑅𝛮𝛥 𝛭𝐷"
    },
    "AUDIO_DATA": {
      "description": "audio package name",
      "required": true,
      "value": "𝑌𝑆 𝛭𝛪𝐷𝛨𝑈𝛮;𝛥𝛲𝛥𝑅𝛮𝛥 𝛭𝐷;https://i.imgur.com/LgaENJd.jpeg"
    },
    "READ_COMMAND": {
      "description": "Read all commands",
      "required": false,
      "value": "false"
    },
  "ERROR_MESSAGE": {
      "description": "get error alert",
      "required": false,
      "value": "true"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ]
}
